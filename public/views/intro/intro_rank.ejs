<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <title><%= data.title %></title>
  <link rel="stylesheet" href="/css/intro.css">
</head>
<body>

<div class="intro-container">
  <h1 class="page-title"><%= data.title %></h1>

  <!-- 고위직 -->
  <div class="rank-section">
    <h2 class="rank-title high">고위직</h2>
    <div class="rank-card">
      <% Object.keys(data.high).forEach(rank => { %>
        <p><strong><%= rank %></strong> <%= data.high[rank] %></p>
      <% }) %>
    </div>
  </div>

  <!-- 간부직 -->
  <div class="rank-section">
    <h2 class="rank-title mid">간부직</h2>
    <div class="rank-card">
      <% Object.keys(data.mid).forEach(rank => { %>
        <p><strong><%= rank %></strong> <%= data.mid[rank] %></p>
      <% }) %>
    </div>
  </div>

  <!-- 일반직 -->
  <div class="rank-section">
    <h2 class="rank-title normal">일반직</h2>

    <% Object.keys(data.normal).forEach(rank => { %>
      <div class="rank-card">
        <strong><%= rank %></strong>
        <ul>
          <% data.normal[rank].forEach(name => { %>
            <li><%= name %></li>
          <% }) %>
        </ul>
      </div>
    <% }) %>
  </div>

  <!-- 시보 -->
  <div class="rank-section">
    <h2 class="rank-title">시보</h2>
    <div class="rank-card">
      <% data.probation.forEach(name => { %>
        <p><%= name %></p>
