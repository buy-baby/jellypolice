<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <title><%= data.title %></title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>

  <div class="container">
    <h2><%= data.title %></h2>

    <% data.ranks.forEach(group => { %>
      <h3><%= group.group %></h3>
      <ul>
        <% group.items.forEach(item => { %>
          <li><%= item %></li>
        <% }) %>
      </ul>
    <% }) %>
  </div>

</body>
</html>
